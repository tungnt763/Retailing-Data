{
    "tables": [
      {
        "file_name": "customers",
        "table_name": "dim_customers",
        "primary_key": ["cust_id"],
        "fields": {
          "cust_id": {
            "column_name": "Customer ID",
            "type": "string",
            "not_null": true,
            "unique": true,
            "example": "1",
            "description": "Unique customer identifier. Must be unique and not null."
          },
          "cust_name": {
            "column_name": "Name",
            "type": "string",
            "not_null": true,
            "example": "Tyler Garcia"
          },
          "cust_email": {
            "column_name": "Email",
            "type": "string",
            "not_null": false,
            "format": "email",
            "example": "tyler.garcia@example.com"
          },
          "cust_phone": {
            "column_name": "Telephone",
            "type": "string",
            "not_null": true,
            "format": "phone_global",
            "example": "+1-958-729-6169"
          },
          "cust_city": {
            "column_name": "City",
            "type": "string",
            "not_null": false,
            "example": "New York"
          },
          "cust_country": {
            "column_name": "Country",
            "type": "string",
            "not_null": false,
            "example": "United States"
          },
          "cust_gender": {
            "column_name": "Gender",
            "type": "string",
            "not_null": false,
            "allowed_values": ["M", "F", "O", "U"],
            "example": "M",
            "description": "M=Male, F=Female, O=Other, U=Unknown"
          },
          "cust_dob": {
            "column_name": "Date Of Birth",
            "type": "date",
            "not_null": false,
            "format": "yyyy-mm-dd",
            "min": "1900-01-01",
            "max": "today",
            "example": "2003-07-15"
          },
          "cust_job_title": {
            "column_name": "Job Title",
            "type": "string",
            "not_null": false,
            "example": "Records manager"
          }
        }
      },
      {
        "file_name": "discounts",
        "table_name": "dim_discounts",
        "primary_key": ["disc_id"],
        "fields": {
          "disc_start_date": {
            "column_name": "Start",
            "type": "date",
            "not_null": true,
            "format": "yyyy-mm-dd",
            "min": "2000-01-01",
            "max": "2100-01-01",
            "example": "2020-01-01"
          },
          "disc_end_date": {
            "column_name": "End",
            "type": "date",
            "not_null": false,
            "format": "yyyy-mm-dd",
            "min": "2000-01-01",
            "max": "2100-01-01",
            "example": "2020-01-10"
          },
          "disc_value": {
            "column_name": "Discont",
            "type": "float",
            "not_null": true,
            "min": 0.0,
            "max": 1.0,
            "example": 0.4
          },
          "disc_desc": {
            "column_name": "Description",
            "type": "string",
            "not_null": false
          },
          "disc_cat": {
            "column_name": "Category",
            "type": "string",
            "not_null": false,
            "example": "Feminine"
          },
          "disc_sub_cat": {
            "column_name": "Sub Category",
            "type": "string",
            "not_null": false,
            "example": "Coats and Blazers"
          }
        },
        "row_checks": [
          {
            "check": "disc_end_date >= disc_start_date",
            "description": "Discount end date must not be before start date"
          }
        ]
      },
      {
        "file_name": "employees",
        "table_name": "dim_employees",
        "primary_key": ["emp_id"],
        "fields": {
          "emp_id": {
            "column_name": "Employee ID",
            "type": "string",
            "not_null": true,
            "unique": true,
            "example": "1"
          },
          "store_id": {
            "column_name": "Store ID",
            "type": "string",
            "not_null": true,
            "foreign_key": {
              "table": "dim_stores",
              "field": "store_id"
            },
            "example": "1"
          },
          "emp_name": {
            "column_name": "Name",
            "type": "string",
            "not_null": true,
            "example": "Stephen Johnson"
          },
          "emp_pos": {
            "column_name": "Position",
            "type": "string",
            "not_null": false,
            "example": "Sales Associate"
          }
        }
      },
      {
        "file_name": "products",
        "table_name": "dim_products",
        "primary_key": ["prod_id"],
        "fields": {
          "prod_id": {
            "column_name": "Product ID",
            "type": "string",
            "not_null": true,
            "unique": true,
            "example": "1"
          },
          "prod_cat": {
            "column_name": "Category",
            "type": "string",
            "not_null": true,
            "allowed_values": ["Feminine", "Masculine", "Children"],
            "example": "Feminine"
          },
          "prod_sub_cat": {
            "column_name": "Sub Category",
            "type": "string",
            "not_null": false,
            "example": "Coats and Blazers"
          },
          "prod_desc_pt": { "column_name": "Description PT", "type": "string", "not_null": false },
          "prod_desc_de": { "column_name": "Description DE", "type": "string", "not_null": false },
          "prod_desc_fr": { "column_name": "Description FR", "type": "string", "not_null": false },
          "prod_desc_es": { "column_name": "Description ES", "type": "string", "not_null": false },
          "prod_desc_en": { "column_name": "Description EN", "type": "string", "not_null": false },
          "prod_desc_zh": { "column_name": "Description ZH", "type": "string", "not_null": false },
          "prod_color": { "column_name": "Color", "type": "string", "not_null": false },
          "prod_size": { "column_name": "Sizes", "type": "string", "not_null": false, "example": "S|M|L|XL" },
          "prod_cost": { "column_name": "Production Cost", "type": "float", "not_null": true, "min": 0 }
        }
      },
      {
        "file_name": "stores",
        "table_name": "dim_stores",
        "primary_key": ["store_id"],
        "fields": {
          "store_id": {
            "column_name": "Store ID",
            "type": "string",
            "not_null": true,
            "unique": true,
            "example": "1"
          },
          "store_country": {
            "column_name": "Country",
            "type": "string",
            "not_null": false,
            "example": "United States"
          },
          "store_city": {
            "column_name": "City",
            "type": "string",
            "not_null": false,
            "example": "New York"
          },
          "store_name": {
            "column_name": "Store Name",
            "type": "string",
            "not_null": true,
            "example": "Store New York"
          },
          "store_emp_num": {
            "column_name": "Number of Employees",
            "type": "int",
            "not_null": false,
            "min": 1,
            "max": 1000,
            "example": 10
          },
          "store_zip": {
            "column_name": "ZIP Code",
            "type": "string",
            "not_null": false,
            "format": "zip_global",
            "example": "10001"
          },
          "store_lat": {
            "column_name": "Latitude",
            "type": "float",
            "not_null": true,
            "min": -90,
            "max": 90,
            "example": 40.7128
          },
          "store_long": {
            "column_name": "Longitude",
            "type": "float",
            "not_null": true,
            "min": -180,
            "max": 180,
            "example": -74.006
          }
        }
      },
      {
        "file_name": "transactions",
        "table_name": "fact_transactions",
        "primary_key": ["trans_id"],
        "fields": {
          "trans_id": {
            "column_name": "Invoice ID",
            "type": "string",
            "not_null": true,
            "unique": true,
            "example": "INV-US-001-03558761"
          },
          "line": {
            "column_name": "Line",
            "type": "int",
            "not_null": true,
            "min": 1,
            "example": 1
          },
          "cust_id": {
            "column_name": "Customer ID",
            "type": "string",
            "not_null": true,
            "foreign_key": {
              "table": "dim_customers",
              "field": "cust_id"
            },
            "example": "47162"
          },
          "prod_id": {
            "column_name": "Product ID",
            "type": "string",
            "not_null": true,
            "foreign_key": {
              "table": "dim_products",
              "field": "prod_id"
            }
          },
          "size": {
            "column_name": "Size",
            "type": "string",
            "not_null": false,
            "example": "M"
          },
          "color": {
            "column_name": "Color",
            "type": "string",
            "not_null": false,
            "example": "NEUTRAL"
          },
          "unit_price": {
            "column_name": "Unit Price",
            "type": "float",
            "not_null": true,
            "min": 0
          },
          "quantity": {
            "column_name": "Quantity",
            "type": "int",
            "not_null": true,
            "min": 0
          },
          "trans_date": {
            "column_name": "Date",
            "type": "datetime",
            "not_null": true,
            "format": "yyyy-mm-dd HH:MM:SS"
          },
          "disc_value": {
            "column_name": "Discount",
            "type": "float",
            "not_null": false,
            "min": 0.0,
            "max": 1.0,
            "example": 0.4
          },
          "total_line": {
            "column_name": "Line Total",
            "type": "float",
            "not_null": true,
            "min": 0
          },
          "store_id": {
            "column_name": "Store ID",
            "type": "string",
            "not_null": true,
            "foreign_key": {
              "table": "dim_stores",
              "field": "store_id"
            }
          },
          "emp_id": {
            "column_name": "Employee ID",
            "type": "string",
            "not_null": false,
            "foreign_key": {
              "table": "dim_employees",
              "field": "emp_id"
            }
          },
          "currency": {
            "column_name": "Currency",
            "type": "string",
            "not_null": true,
            "allowed_values": ["USD", "CNY", "EUR", "VND", "GBP"],
            "example": "USD"
          },
          "currency_sym": {
            "column_name": "Currency Symbol",
            "type": "string",
            "not_null": false,
            "example": "$"
          },
          "sku": {
            "column_name": "SKU",
            "type": "string",
            "not_null": false
          },
          "trans_type": {
            "column_name": "Transaction Type",
            "type": "string",
            "not_null": false,
            "allowed_values": ["Sale", "Refund", "Exchange"]
          },
          "pay_method": {
            "column_name": "Payment Method",
            "type": "string",
            "not_null": false,
            "allowed_values": ["Cash", "Credit Card", "Transfer", "E-wallet"]
          },
          "trans_total": {
            "column_name": "Invoice Total",
            "type": "float",
            "not_null": true,
            "min": 0
          }
        },
        "row_checks": [
          {
            "check": "total_line == unit_price * quantity * (1 - disc_value)",
            "description": "Each line total must match unit price * quantity * (1-discount)"
          }
        ]
      }
    ],
    "global_formats": {
      "email": "^[\\w\\.-]+@[\\w\\.-]+\\.[a-zA-Z]{2,}$",
      "phone_global": "^[+]?\\d[\\d .\\-x()]{7,}$",
      "zip_global": "^[A-Za-z0-9\\- ]{3,10}$"
    },
    "global_countries": ["United States", "China", "United Kingdom", "Vietnam", "France", "Germany", "Japan", "Canada", "Brazil", "India"],
    "global_currency_codes": ["USD", "CNY", "EUR", "VND", "GBP", "JPY", "CAD", "BRL", "INR"],
    "global_genders": ["M", "F", "O", "U"],
    "date_formats": ["yyyy-mm-dd", "yyyy-mm-dd HH:MM:SS"]
}
  